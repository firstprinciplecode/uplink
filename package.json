{
  "name": "uplink-cli",
  "version": "0.1.26",
  "description": "Expose localhost to the internet in seconds. Interactive terminal UI, permanent custom domains, zero config. A modern ngrok alternative.",
  "keywords": [
    "tunnel",
    "localhost",
    "ngrok",
    "ngrok-alternative",
    "expose",
    "cli",
    "dev-tools",
    "webhook",
    "public-url",
    "port-forwarding",
    "reverse-proxy",
    "tunneling",
    "local-development",
    "https-tunnel",
    "custom-domain"
  ],
  "homepage": "https://uplink.spot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstprinciplecode/uplink.git"
  },
  "bugs": {
    "url": "https://github.com/firstprinciplecode/uplink/issues"
  },
  "author": "First Principle Code",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "uplink": "./cli/bin/uplink.js"
  },
  "files": [
    "cli/",
    "scripts/tunnel/client-improved.js",
    "README.md",
    "assets/"
  ],
  "scripts": {
    "dev:relay": "node scripts/tunnel/relay.js",
    "dev:stub": "PORT=4100 AGENTCLOUD_TOKEN_DEV=dev-token node scripts/dev/stub-control-plane.js",
    "dev:cli": "tsx cli/src/index.ts",
    "dev:api": "tsx backend/src/server.ts",
    "migrate": "tsx backend/scripts/migrate.ts",
    "smoke:tunnel": "bash scripts/tunnel-smoke.sh",
    "smoke:db": "bash scripts/db-api-smoke.sh",
    "smoke:all": "bash scripts/smoke-all.sh",
    "test:features": "bash scripts/test-new-features.sh",
    "test:comprehensive": "bash scripts/test-comprehensive.sh"
  },
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "body-parser": "^1.20.3",
    "commander": "^12.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.12.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^22.7.4",
    "@types/node-fetch": "^2.6.11",
    "typescript": "^5.6.2"
  }
}
